# Core Supabase configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=service-role-key

# Frontend / GraphQL shared URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Mesh gateway
MESH_GATEWAY_PORT=4100

# GraphQL services
USER_GRAPHQL_MS_PORT=4102
PLAN_GRAPHQL_MS_PORT=4103

# Other services
UPLOAD_MS_PORT=4200

# Auth.js Configuration
AUTH_SECRET=generate-with-openssl-rand-base64-32
INTERNAL_API_KEY=generate-with-openssl-rand-base64-32
# User service encode/decode token before saving in db
TOKEN_ENCRYPTION_KEY=generate-with-openssl-rand-base64-32