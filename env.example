# Core Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=public-anon-key
SUPABASE_SERVICE_ROLE_KEY=service-role-key

# Frontend / GraphQL shared URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Split GraphQL services (user/plan)
USER_GRAPHQL_MS_PORT=4101
PLAN_GRAPHQL_MS_PORT=4102

# Mesh gateway
MESH_PORT=4103
MESH_PUBLIC_GRAPHQL_URL=http://localhost:4103/graphql
MESH_UPLOAD_ADDRESS=localhost:50052
MESH_UPLOAD_PROTO_PATH=./packages/shared/proto/upload/v1/upload.proto

